  <oa:Annotation rdf:about="http://www.essepuntato.it/2013/citalo/test/data/user3/46">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <oa:annotatedBy rdf:resource="http://www.essepuntato.it/2013/citalo/test/data/user3"/>
    <oa:hasTarget>
      <frbr:Expression rdf:about="http://www.essepuntato.it/2013/citalo/test/data/46">
        <c4o:hasContent>Especially the work done by X is of interest, since they have shown that a data structure based on RDF, called EARMARK (Extreme Annotational RDF Markup), not only fully supports the expressiveness of GODDAGs but additionally introduces a new sub-type, called e-GODDAG (extended GODDAG) that adds anonymous non-terminal nodes (for establishing multiple arcs between two nodes and therefore allowing repetitive structures).</c4o:hasContent>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
        <dcterms:identifier>46</dcterms:identifier>
      </frbr:Expression>
    </oa:hasTarget>
    <oa:hasBody rdf:resource="http://purl.org/spar/cito/obtainsBackgroundFrom"/>
  </oa:Annotation>
  <owl:NamedIndividual rdf:about="http://www.essepuntato.it/2013/citalo/test/data/user1/55">
