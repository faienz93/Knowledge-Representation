    <oa:hasBody rdf:resource="http://purl.org/spar/cito/citesAsDataSource"/>
  </owl:NamedIndividual>
  <owl:NamedIndividual rdf:about="http://www.essepuntato.it/2013/citalo/test/data/user3/2">
    <oa:annotatedBy rdf:resource="http://www.essepuntato.it/2013/citalo/test/data/user3"/>
    <oa:hasBody rdf:resource="http://purl.org/spar/cito/usesConclusionsFrom"/>
    <oa:hasTarget>
      <frbr:Expression rdf:about="http://www.essepuntato.it/2013/citalo/test/data/2">
        <dcterms:identifier>2</dcterms:identifier>
        <c4o:hasContent>But in a 2009 talk X Norm Walsh presented the results of a study counting which steps were used in XProc pipelines. He didn't care what the pipelines did, only what steps (i.e., XML element types) they used. For his purposes, any valid XProc document was interoperational.</c4o:hasContent>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
      </frbr:Expression>
    </oa:hasTarget>
    <rdf:type rdf:resource="http://www.w3.org/ns/oa#Annotation"/>
  </owl:NamedIndividual>
