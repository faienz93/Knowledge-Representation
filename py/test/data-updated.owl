  <oa:Annotation rdf:about="http://www.essepuntato.it/2013/citalo/test/data/user3/53">
    <oa:hasTarget rdf:resource="http://www.essepuntato.it/2013/citalo/test/data/53"/>
    <oa:annotatedBy rdf:resource="http://www.essepuntato.it/2013/citalo/test/data/user3"/>
