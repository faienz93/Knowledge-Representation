  </owl:NamedIndividual>
  <oa:Annotation rdf:about="http://www.essepuntato.it/2013/citalo/test/data/user1/20">
    <oa:hasBody rdf:resource="http://purl.org/spar/cito/usesDataFrom"/>
    <oa:annotatedBy rdf:resource="http://www.essepuntato.it/2013/citalo/test/data/user1"/>
    <oa:hasTarget>
      <owl:NamedIndividual rdf:about="http://www.essepuntato.it/2013/citalo/test/data/20">
        <c4o:hasContent>We use the XMark XQuery queries numbered q1 to q20, and synthetically generate several XML data files from X, these being of sizes in the range 1MB to 64MB.</c4o:hasContent>
        <dcterms:identifier>20</dcterms:identifier>
        <rdf:type rdf:resource="http://purl.org/vocab/frbr/core#Expression"/>
      </owl:NamedIndividual>
    </oa:hasTarget>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  </oa:Annotation>
  <oa:Annotation rdf:about="http://www.essepuntato.it/2013/citalo/test/data/user1/16">
    <oa:hasTarget>
      <owl:NamedIndividual rdf:about="http://www.essepuntato.it/2013/citalo/test/data/16">
        <dcterms:identifier>16</dcterms:identifier>
        <rdf:type rdf:resource="http://purl.org/vocab/frbr/core#Expression"/>
        <c4o:hasContent>There are other Java class manipulation tools which we only mention here such as SERP, JIOE: these are described in X.</c4o:hasContent>
      </owl:NamedIndividual>
    </oa:hasTarget>
    <oa:annotatedBy rdf:resource="http://www.essepuntato.it/2013/citalo/test/data/user1"/>
