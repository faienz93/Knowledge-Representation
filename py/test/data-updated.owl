  <oa:Annotation rdf:about="http://www.essepuntato.it/2013/citalo/test/data/user1/15">
    <oa:hasBody>
      <owl:NamedIndividual rdf:about="http://purl.org/spar/cito/citesAsSourceDocument"/>
    </oa:hasBody>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
