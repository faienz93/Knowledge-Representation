    <oa:hasBody rdf:resource="http://purl.org/spar/cito/citesForInformation"/>
  </oa:Annotation>
  <oa:Annotation rdf:about="http://www.essepuntato.it/2013/citalo/test/data/user1/99">
    <oa:hasTarget>
      <frbr:Expression rdf:about="http://www.essepuntato.it/2013/citalo/test/data/99">
        <c4o:hasContent>We have already defined a linguistic ontology from the TFS provided by linguists for prosody, phonetics, lexical and disfluency domains. An applicative framework is under development, it is based on semantic web proposals such as OWL DL for the representation of this ontology and SPARQL X the querying language of semantic web for its manipulation.</c4o:hasContent>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
        <dcterms:identifier>99</dcterms:identifier>
      </frbr:Expression>
    </oa:hasTarget>
    <oa:hasBody rdf:resource="http://purl.org/spar/cito/citesAsSourceDocument"/>
