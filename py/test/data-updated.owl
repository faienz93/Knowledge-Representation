  <owl:NamedIndividual rdf:about="http://www.essepuntato.it/2013/citalo/test/data/user2/42">
    <oa:hasTarget>
      <frbr:Expression rdf:about="http://www.essepuntato.it/2013/citalo/test/data/42">
        <c4o:hasContent>Another alternative formal model for markup languages that has received much attention is the General Ordered-Descendant Directed Acyclic Graph (GODDAG) (see X for a more recent discussion).</c4o:hasContent>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
