    <oa:hasTarget>
      <owl:NamedIndividual rdf:about="http://www.essepuntato.it/2013/citalo/test/data/19">
        <c4o:hasContent>We used Saxon 9.3.0.4 as the baseline. The test machine was a Intel Core i5 processor 430M laptop with 4GB memory, 2.26Ghz CPU and 3MB L3 cache, running Ubuntu 10.04LTS Linux. The compiler was Sun/Oracle Java 1.6.0.2. The experiments are based on the XMark benchmark X.</c4o:hasContent>
        <rdf:type rdf:resource="http://purl.org/vocab/frbr/core#Expression"/>
        <dcterms:identifier>19</dcterms:identifier>
      </owl:NamedIndividual>
    </oa:hasTarget>
