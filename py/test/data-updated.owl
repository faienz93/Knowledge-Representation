  </oa:Annotation>
  <owl:NamedIndividual rdf:about="http://www.essepuntato.it/2013/citalo/test/data/user3/0">
    <oa:hasTarget rdf:resource="http://www.essepuntato.it/2013/citalo/test/data/0"/>
    <oa:annotatedBy rdf:resource="http://www.essepuntato.it/2013/citalo/test/data/user3"/>
